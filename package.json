    "keywords": [
        "promise",
        "chain",
        "io"
    ],
    "engines": {
        "node": ">= 0.8"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:jnewman/compromise.git"
    },
    "bugs": {
        "url": "https://github.com/jnewman/compromise/issues"
    },
    "main": "./src/compromise",
